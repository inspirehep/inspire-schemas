{
    "$schema": "http://json-schema.org/schema#",
    "noAdditionalProperties": true,
    "properties": {
        "date": {
            "type": "string"
        },
        "email": {
            "type": "string"
        },
        "method": {
            "enum": [
                "submitter",
                "oai",
                "batchuploader",
                "hepcrawl"
            ],
            "type": "string"
        },
        "orcid": {
            "pattern": "^\\d{4}-\\d{4}-\\d{4}-\\d{3}[0-9X]$",
            "type": "string"
        },
        "source": {
            "type": "string"
        },
        "submission_number": {
            "type": "string"
        }
    },
    "type": "object"
}
